{
    "java.project.sourcePaths": [
        "src"
    ],
    "java.project.referencedLibraries": [
        "lib/javafx.web.jar",
        "lib/javafx.swing.jar",
        "lib/javafx.media.jar",
        "lib/javafx.graphics.jar",
        "lib/javafx.fxml.jar",
        "lib/javafx.controls.jar",
        "lib/javafx.base.jar",
        "lib/javafx-swt.jar",
        "lib/junit-jupiter-api-5.9.3.jar",
        "lib/junit-jupiter-engine-5.9.3.jar",
        "lib/junit-jupiter-params-5.9.3.jar",
        "lib/junit-platform-commons-1.9.3.jar",
        "lib/junit-platform-engine-1.9.3.jar",
        "lib/junit-platform-launcher-1.9.3.jar",
        "lib/opentest4j-1.2.0.jar",
        "lib/apiguardian-api-1.1.2.jar"
    ],
    "java.test.config": {
        "workingDirectory": "${workspaceFolder}"
    },
    "java.test.defaultConfig": "default",
    "java.test.configurations": [
        {
            "name": "default",
            "workingDirectory": "${workspaceFolder}"
        }
    ],
    "java.test.editor.enableShortcuts": true,
    "java.test.includes": [
        "**/*Test.java",
        "**/*Tests.java",
        "**/*TestCase.java"
    ],
    "java.debug.settings.console": "internalConsole",
    "java.test.packagePath": "src"
}